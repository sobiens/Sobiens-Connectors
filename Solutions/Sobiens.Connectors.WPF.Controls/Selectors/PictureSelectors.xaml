<wpfc:HostControl x:Class="Sobiens.Connectors.WPF.Controls.Selectors.PictureSelectors"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:wpfc="clr-namespace:Sobiens.Connectors.WPF.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="154" d:DesignWidth="341">
    <Grid>
        <ListBox Height="133" HorizontalAlignment="Left" Margin="10,10,0,0" Name="PicturesListBox" Tag="SiteSettings" VerticalAlignment="Top" Width="227" SelectedValuePath="ID" DisplayMemberPath="Name"/>
        <Button Content="{DynamicResource New}" Height="23" HorizontalAlignment="Left" Margin="243,91,0,0" Name="NewButton" VerticalAlignment="Top" Width="89" Click="NewButton_Click" />
        <Button Content="{DynamicResource Delete}" Height="23" HorizontalAlignment="Left" Margin="243,0,0,12" Name="DeleteButton" VerticalAlignment="Bottom" Width="89" Click="DeleteButton_Click" />
    </Grid>
</wpfc:HostControl>
